#include <iostream>
using namespace std;

class node{
    public:
    int data;
    node* prev;
    node* next;

    node(int d){
        this -> data = d;
        this -> prev = NULL;
        this -> next = NULL;
    }
};

void print(node* head){

    node* temp = head;

    while(temp != NULL){
        cout<<temp -> data<<"->";
        temp = temp -> next;
    }
    cout<<"NULL"<<endl;
}

// TO find lenght of Doubly linked list
int getlenght(node* head){
    int len = 0;
  node* temp = head;

    while(temp != NULL){
        len++;
        cout<<temp -> data<<"->";
        temp = temp -> next;
    }
    cout<<"NULL"<<endl;
return len;
}

//insert At head
void insertAthead(node* head,int d){
    node* temp = new node(d);
    temp->next = head;
    head->prev = temp;
}

int main() {

    node* node1 = new node(10);
    node* head = node1;

    print(head);
    cout<<getlenght(head)<<endl;

    insertAthead(head,9);
    print(head);

}
